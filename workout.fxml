<AnchorPane fx:id="workoutbg" prefHeight="488.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.workoutController">
    <children>
        <Label alignment="CENTER" layoutX="257.0" layoutY="27.0" prefHeight="30.0" prefWidth="113.0" text="Workout Tracker" />
      <TextField fx:id="workoutInput" layoutX="42.0" layoutY="99.0" prefHeight="78.0" prefWidth="278.0" promptText="Enter Workout Here (Pushups, Situps, Squats, etc.)" />
      <TextField fx:id="reps" alignment="CENTER" layoutX="337.0" layoutY="122.0" prefHeight="37.0" prefWidth="87.0" promptText="0" />
      <TextField fx:id="sets" alignment="CENTER" layoutX="463.0" layoutY="122.0" prefHeight="38.0" prefWidth="87.0" promptText="0" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="345.0" layoutY="99.0" prefHeight="18.0" prefWidth="78.0" text="Reps" />
      <Label alignment="CENTER" layoutX="425.0" layoutY="127.0" prefHeight="38.0" prefWidth="39.0" text="X">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="462.0" layoutY="100.0" prefHeight="18.0" prefWidth="78.0" text="Sets" />
      <ListView layoutX="42.0" layoutY="196.0" prefHeight="266.0" prefWidth="381.0" />
