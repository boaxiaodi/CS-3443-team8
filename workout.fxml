      <Button fx:id="removeWorkoutBtn" alignment="CENTER" layoutX="438.0" layoutY="305.0" mnemonicParsing="false" onAction="#removeWorkout" prefHeight="38.0" prefWidth="139.0" text="Remove Workout" />
      <Button fx:id="backbtn" alignment="CENTER" layoutX="437.0" layoutY="396.0" mnemonicParsing="false" onAction="#backBtn" prefHeight="38.0" prefWidth="139.0" text="Back" />
    </children>
</AnchorPane>
