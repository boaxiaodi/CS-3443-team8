<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane dividerPositions="0.5" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.calendarController">
   <items>
      <AnchorPane prefHeight="36.0" prefWidth="633.0">
         <children>
            <Button fx:id="addButton" layoutX="14.0" layoutY="5.0" mnemonicParsing="false" onAction="#addEvent" text="Add Event" />
            <DatePicker fx:id="connectDate" layoutX="104.0" layoutY="7.0" onAction="#ShowDate" showWeekNumbers="true" />
            <TextField fx:id="descriptionTextField" layoutX="302.0" layoutY="7.0" prefHeight="25.0" prefWidth="320.0" promptText="Add Desription of Event here..." />
            <Separator layoutX="91.0" orientation="VERTICAL" prefHeight="36.0" prefWidth="6.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="91.0" AnchorPane.topAnchor="0.0" />
            <Separator layoutX="286.0" orientation="VERTICAL" prefHeight="36.0" prefWidth="6.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="286.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <ListView fx:id="eventList" prefHeight="200.0" prefWidth="200.0" />
   </items>
</SplitPane>
